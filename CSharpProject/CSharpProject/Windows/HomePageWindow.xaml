<Window x:Class="CSharpProject.Windows.HomePageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpProject.Windows"
        mc:Ignorable="d"
        Title="HomePageWindow" Height="450" Width="400" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,2,0">
        <Button x:Name="CreateManager" Content="CreateManager" HorizontalAlignment="Center" Margin="0,33,0,0" VerticalAlignment="Top" Height="61" Width="318" Click="CreateManager_Click"/>
        <Button x:Name="CreateBook" Content="CreateBook" HorizontalAlignment="Center" Margin="0,119,0,0" VerticalAlignment="Top" Height="61" Width="318" Click="CreateBook_Click"/>
        <Button x:Name="CreateCustomer" Content="CreateCustomer" HorizontalAlignment="Left" Margin="37,203,0,0" VerticalAlignment="Top" Height="61" Width="318" Click="CreateCustomer_Click"/>

    </Grid>
</Window>
